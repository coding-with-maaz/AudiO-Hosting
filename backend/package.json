{
  "name": "audio-hosting-platform",
  "version": "1.0.0",
  "description": "Backend for audio hosting platform with affiliate marketing and pricing plans",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "create-admin": "node scripts/create-admin.js",
    "kill-port": "node scripts/kill-port.js"
  },
  "keywords": [
    "audio",
    "hosting",
    "affiliate",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "cookie-parser": "^1.4.6",
    "archiver": "^6.0.1",
    "nodemailer": "^6.9.7",
    "bull": "^4.12.0",
    "redis": "^4.6.11",
    "axios": "^1.6.2",
    "csv-writer": "^1.6.0",
    "pdfkit": "^0.14.0",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}

